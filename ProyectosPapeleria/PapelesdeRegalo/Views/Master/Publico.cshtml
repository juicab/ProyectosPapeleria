            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">

                    <li class="nav-item prod"><a href="@Url.Action("Granmark","Papeles")" class="nav-link"><b>Granmark</b></a></li>
                    
                    <li class="nav-item qui"><a href="@Url.Action("Galas","Papeles")" class="nav-link"><b>Galas</b></a></li>
