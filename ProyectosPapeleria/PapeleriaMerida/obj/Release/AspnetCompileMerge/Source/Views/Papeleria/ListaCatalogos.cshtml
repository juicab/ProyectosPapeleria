@using System.Data;
@model DataTable
@foreach (DataRow item in Model.Rows)
{
    <div class="col-md-6 col-lg-2 ftco-animate">
        <div class="staff">
            <a href="~/admin/docs/@item["docuCat"]" download="Catálogo @item["nombreCat"] @item["fechaCat"]" class="img-wrap d-flex align-items-stretch">
                <div class="img align-self-stretch" style="background-image: url(../../images/DescargaPDF.png);"></div>
            </a>
            <div class="text pt-3 text-center">
                <h3>@item["nombreCat"]</h3>
                <div class="faded">
                    <p>@item["fechaCat"]</p>
                </div>
            </div>
        </div>
    </div>
}
